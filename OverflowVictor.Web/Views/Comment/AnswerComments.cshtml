@model IEnumerable<OverflowVictor.Web.Models.CommentModel>

<ul>
    @foreach (var item in Model)
    {
        <li>@item.Comment by @item.OwnerName Created on @item.CreationTime</li>
    }
</ul>
<div class="form-group">
    <label class="col-md-4 control-label" for="comment">Comment</label>
    <div class="col-md-4">
        <input id="comment" name="comment" type="text" placeholder="Add Comment" class="form-control input-md" required="">

    </div>
</div>